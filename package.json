{
  "name": "vuesion",
  "private": true,
  "version": "4.8.1",
  "description": "Vuesion is a boilerplate that helps product teams build faster than ever with fewer headaches and modern best practices across engineering & design.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesion/vuesion.git"
  },
  "keywords": [
    "vue",
    "vite",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "vitetest",
    "enterprise",
    "components",
    "design-system",
    "nuxt.js",
    "nuxt",
    "apollo",
    "composition-api",
    "GraphQL"
  ],
  "author": "Johannes Werner <johannes.werner@hey.com>",
  "license": "MIT",
  "homepage": "https://github.com/vuesion/vuesion",
  "bugs": {
    "url": "https://github.com/vuesion/vuesion/issues"
  },
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu nuxt dev -o",
    "storybook:dev": "rm -rf node_modules/.cache/storybook && start-storybook -p 6006",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu vitest run --coverage",
    "generate": "vuesion generate",
    "type-gen:gql": "graphql-codegen",
    "type-gen:swagger": "openapi-typescript https://petstore.swagger.io/v2/swagger.json --output src/interfaces/swagger.ts",
    "extract-i18n-messages": "vuesion em -s -u -t",
    "test:watch": "cross-env NODE_ICU_DATA=node_modules/full-icu vuesion test --coverage --watch",
    "e2e": "vuesion e2e",
    "lint": "eslint . --ext ts,vue --fix --cache",
    "clean": "vuesion clean",
    "storybook:build": "build-storybook",
    "update": "vuesion update",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore --write ./**/*.{ts,vue,md,json,js,yml} --loglevel error",
    "release:major": "vuesion release --major",
    "release:minor": "vuesion release --minor",
    "release:patch": "vuesion release --patch",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "build:spa": "nuxt generate",
    "start": "cross-env NODE_ICU_DATA=node_modules/full-icu nuxt start",
    "ci": "vuesion parallel \"npm run lint\" \"npm run test -- --runInBand\"",
    "postci": "vuesion parallel \"npm run build\" \"npm run storybook:build\"",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged && concurrently \"npm run lint\" \"npm test\""
  },
  "dependencies": {
    "@vee-validate/rules": "4.5.11",
    "@vueuse/core": "8.6.0",
    "animejs": "3.2.1",
    "cross-env": "7.0.3",
    "full-icu": "1.4.0",
    "marked": "4.0.17",
    "mitt": "3.0.0",
    "vee-validate": "4.5.11"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@commitlint/config-conventional": "17.0.2",
    "@nuxtjs/color-mode": "3.1.4",
    "@nuxtjs/eslint-config-typescript": "10.0.0",
    "@storybook/addon-actions": "6.5.6",
    "@storybook/addon-essentials": "6.5.6",
    "@storybook/addon-interactions": "6.5.6",
    "@storybook/addon-links": "6.5.6",
    "@storybook/builder-vite": "0.1.36",
    "@storybook/testing-library": "0.0.11",
    "@storybook/vue3": "6.5.6",
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/vue": "6.6.0",
    "@types/animejs": "3.1.4",
    "@types/marked": "4.0.3",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vitejs/plugin-vue": "2.3.3",
    "@vuesion/storybook-theme-switcher": "4.0.0-beta.4",
    "babel-loader": "8.2.5",
    "c8": "7.11.3",
    "concurrently": "7.2.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-storybook": "0.5.12",
    "eslint-plugin-vue": "9.1.1",
    "flush-promises": "1.0.2",
    "happy-dom": "5.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "lodash": "4.17.21",
    "nuxt": "3.0.0-rc.4",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "sass": "1.52.2",
    "sass-loader": "13.0.0",
    "typescript": "4.7.3",
    "vite-plugin-eslint": "1.6.1",
    "vitest": "0.14.2",
    "vue-loader": "16.8.3"
  }
}
