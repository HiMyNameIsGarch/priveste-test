{
  "name": "vuesion",
  "private": true,
  "version": "4.8.1",
  "description": "Vuesion is a boilerplate that helps product teams build faster than ever with fewer headaches and modern best practices across engineering & design.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesion/vuesion.git"
  },
  "keywords": [
    "vue",
    "vite",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "vitetest",
    "enterprise",
    "components",
    "design-system",
    "nuxt.js",
    "nuxt",
    "apollo",
    "composition-api",
    "GraphQL"
  ],
  "author": "Johannes Werner <johannes.werner@hey.com>",
  "license": "MIT",
  "homepage": "https://github.com/vuesion/vuesion",
  "bugs": {
    "url": "https://github.com/vuesion/vuesion/issues"
  },
  "scripts": {
    "dev": "nuxt dev -o",
    "storybook:dev": "start-storybook -p 6006",
    "test": "vitest run --coverage",
    "lint": "eslint . --ext ts,vue --fix --cache",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore --write ./**/*.{ts,vue,md,json,js,yml} --loglevel error",
    "build": "nuxt build",
    "storybook:build": "build-storybook",
    "pre-render": "nuxt generate",
    "preview": "nuxt preview",
    "start": "nuxt start",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged && concurrently \"npm run lint\" \"npm test\""
  },
  "dependencies": {
    "@vueuse/core": "8.6.0",
    "animejs": "3.2.1"
  },
  "devDependencies": {
    "@babel/core": "7.18.2",
    "@commitlint/config-conventional": "17.0.2",
    "@nuxtjs/color-mode": "3.1.4",
    "@nuxtjs/eslint-config-typescript": "10.0.0",
    "@storybook/addon-actions": "6.5.6",
    "@storybook/addon-essentials": "6.5.6",
    "@storybook/addon-interactions": "6.5.6",
    "@storybook/addon-links": "6.5.6",
    "@storybook/builder-vite": "0.1.36",
    "@storybook/testing-library": "0.0.11",
    "@storybook/vue3": "6.5.6",
    "@testing-library/vue": "6.6.0",
    "@types/animejs": "3.1.4",
    "@typescript-eslint/eslint-plugin": "5.27.1",
    "@typescript-eslint/parser": "5.27.1",
    "@vitejs/plugin-vue": "2.3.3",
    "@vuesion/storybook-theme-switcher": "4.0.0-beta.4",
    "babel-loader": "8.2.5",
    "c8": "7.11.3",
    "concurrently": "7.2.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-storybook": "0.5.12",
    "eslint-plugin-vue": "9.1.1",
    "happy-dom": "5.2.0",
    "html-webpack-plugin": "5.5.0",
    "husky": "8.0.1",
    "lodash": "4.17.21",
    "nuxt": "3.0.0-rc.3",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "sass": "1.52.2",
    "sass-loader": "13.0.0",
    "typescript": "4.7.3",
    "vite-plugin-eslint": "1.6.1",
    "vitest": "0.14.2",
    "vue-loader": "16.8.3"
  }
}
