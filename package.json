{
  "name": "vuesion",
  "private": true,
  "version": "4.8.1",
  "description": "Vuesion is a boilerplate that helps product teams build faster than ever with fewer headaches and modern best practices across engineering & design.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesion/vuesion.git"
  },
  "keywords": [
    "vue",
    "vuex",
    "vue-router",
    "webpack",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "jest",
    "enterprise",
    "components",
    "design-system",
    "nuxt.js",
    "nuxt",
    "apollo",
    "composition-api",
    "GraphQL"
  ],
  "author": "Johannes Werner <johannes.werner@hey.com>",
  "license": "MIT",
  "homepage": "https://github.com/vuesion/vuesion",
  "bugs": {
    "url": "https://github.com/vuesion/vuesion/issues"
  },
  "scripts": {
    "dev": "nuxt dev -o",
    "test": "vitest run --coverage",
    "lint": "eslint . --ext ts,vue --fix --cache",
    "prettier": "prettier --config .prettierrc --ignore-path .prettierignore --write ./**/*.{ts,vue,md,json,js,yml} --loglevel error",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged && concurrently \"npm run lint\" \"npm test\""
  },
  "devDependencies": {
    "@commitlint/config-conventional": "17.0.2",
    "@nuxtjs/color-mode": "3.1.3",
    "@nuxtjs/eslint-config-typescript": "10.0.0",
    "@testing-library/vue": "6.4.2",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "@vitejs/plugin-vue": "2.3.3",
    "c8": "7.11.3",
    "concurrently": "7.2.1",
    "eslint": "8.17.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-vue": "9.1.0",
    "happy-dom": "5.0.0",
    "husky": "8.0.1",
    "nuxt": "3.0.0-rc.3",
    "prettier": "2.6.2",
    "pretty-quick": "3.1.3",
    "sass": "1.52.2",
    "sass-loader": "13.0.0",
    "typescript": "4.7.3",
    "vite-plugin-eslint": "1.6.1",
    "vitest": "0.14.0"
  }
}
