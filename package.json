{
  "name": "vuesion",
  "private": true,
  "version": "4.8.1",
  "description": "Vuesion is a boilerplate that helps product teams build faster than ever with fewer headaches and modern best practices across engineering & design.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesion/vuesion.git"
  },
  "keywords": [
    "vue 3",
    "vite",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "vitetest",
    "enterprise",
    "components",
    "design-system",
    "nuxt 3",
    "composition-api"
  ],
  "author": "Johannes Werner <johannes.werner@hey.com>",
  "license": "MIT",
  "homepage": "https://github.com/vuesion/vuesion",
  "bugs": {
    "url": "https://github.com/vuesion/vuesion/issues"
  },
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu nuxt dev",
    "storybook:dev": "storybook dev -p 6006",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu vitest run --coverage",
    "new:component": "hygen new component",
    "new:store": "hygen new store",
    "new:page": "hygen new page",
    "new:crud-entity": "hygen new crud-entity && npm run db:migrate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "ts-node ./prisma/seed.ts",
    "db:generate-client": "prisma generate",
    "type-gen:swagger": "openapi-typescript https://petstore.swagger.io/v2/swagger.json --output src/interfaces/swagger.ts",
    "extract-i18n-messages": "ts-node ./tools/extract-i18n-messages.ts",
    "test:watch": "cross-env NODE_ICU_DATA=node_modules/full-icu vitest",
    "e2e": "cypress run --headless",
    "lint": "eslint . --ext ts,vue --fix --cache && tsc --project ./tsconfig.json --noEmit",
    "storybook:build": "storybook build",
    "release:major": "ts-node ./tools/release.ts --major",
    "release:minor": "ts-node ./tools/release.ts --minor",
    "release:patch": "ts-node ./tools/release.ts --patch",
    "build": "nuxt build",
    "build:analyze": "nuxt build --analyze",
    "build:spa": "nuxt generate",
    "start": "cross-env NODE_ICU_DATA=node_modules/full-icu node .output/server/index.mjs",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged && concurrently \"npm run lint\" \"npm test\""
  },
  "dependencies": {
    "@pinia/nuxt": "0.4.9",
    "@sidebase/nuxt-prisma": "0.1.2",
    "@vee-validate/rules": "4.8.6",
    "@vueuse/core": "10.1.2",
    "animejs": "3.2.1",
    "bcrypt": "5.1.0",
    "cross-env": "7.0.3",
    "full-icu": "1.5.0",
    "lodash-es": "4.17.21",
    "marked": "5.0.0",
    "mitt": "3.0.0",
    "next-auth": "4.22.1",
    "pinia": "2.0.35",
    "prisma": "4.13.0",
    "vee-validate": "4.8.6"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@commitlint/config-conventional": "17.6.3",
    "@nuxt/devtools": "0.4.5",
    "@nuxt/image-edge": "1.0.0-28050764.aaee7ac",
    "@nuxtjs/color-mode": "3.2.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/html-validator": "1.2.5",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/robots": "3.0.0",
    "@sidebase/nuxt-auth": "0.5.0",
    "@storybook/addon-actions": "7.0.9",
    "@storybook/addon-essentials": "7.0.9",
    "@storybook/addon-interactions": "7.0.9",
    "@storybook/addon-links": "7.0.9",
    "@storybook/addon-mdx-gfm": "7.0.9",
    "@storybook/testing-library": "0.1.0",
    "@storybook/vue3": "7.0.9",
    "@storybook/vue3-vite": "7.0.9",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/vue": "7.0.0",
    "@types/animejs": "3.1.7",
    "@types/bcrypt": "5.0.0",
    "@types/lodash-es": "4.17.7",
    "@types/marked": "4.3.0",
    "@typescript-eslint/eslint-plugin": "5.59.2",
    "@typescript-eslint/parser": "5.59.2",
    "@vite-pwa/nuxt": "0.0.7",
    "@vitejs/plugin-vue": "4.2.1",
    "@vitest/coverage-c8": "0.31.0",
    "@vuesion/storybook-theme-switcher": "5.1.1-beta.0",
    "babel-loader": "9.1.2",
    "c8": "7.13.0",
    "concurrently": "8.0.1",
    "cypress": "12.11.0",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-storybook": "0.6.12",
    "eslint-plugin-vue": "9.11.0",
    "flush-promises": "1.0.2",
    "generate-changelog": "1.8.0",
    "html-webpack-plugin": "5.5.1",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "jsdom": "22.0.0",
    "nuxt": "3.4.3",
    "nuxt-delay-hydration": "1.1.4",
    "nuxt-security": "0.13.0",
    "openapi-typescript": "5.4.1",
    "postcss": "8.4.23",
    "prettier": "2.8.8",
    "prettier-plugin-prisma": "4.13.0",
    "pretty-quick": "3.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "1.62.1",
    "sass-loader": "13.2.2",
    "storybook": "7.0.9",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vite": "4.3.5",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "0.31.0",
    "vue-loader": "17.1.0"
  },
  "peerDependencies": {
    "consola": "3.0.1",
    "vue": "3.2.47"
  }
}
