{
  "name": "vuesion",
  "private": true,
  "version": "4.8.1",
  "description": "Vuesion is a boilerplate that helps product teams build faster than ever with fewer headaches and modern best practices across engineering & design.",
  "repository": {
    "type": "git",
    "url": "https://github.com/vuesion/vuesion.git"
  },
  "keywords": [
    "vue 3",
    "vite",
    "typescript",
    "server-side-rendering",
    "boilerplate",
    "vitetest",
    "enterprise",
    "components",
    "design-system",
    "nuxt 3",
    "composition-api"
  ],
  "author": "Johannes Werner <johannes.werner@hey.com>",
  "license": "MIT",
  "homepage": "https://github.com/vuesion/vuesion",
  "bugs": {
    "url": "https://github.com/vuesion/vuesion/issues"
  },
  "scripts": {
    "dev": "cross-env NODE_ICU_DATA=node_modules/full-icu nuxi dev",
    "storybook:dev": "rm -rf ./node_modules/.cache && storybook dev -p 6006",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu vitest run --coverage",
    "new:component": "hygen new component",
    "new:store": "hygen new store",
    "new:page": "hygen new page",
    "new:crud-entity": "hygen new crud-entity && npm run db:migrate",
    "type-gen:swagger": "openapi-typescript https://petstore.swagger.io/v2/swagger.json --output src/interfaces/swagger.ts",
    "extract-i18n-messages": "ts-node ./tools/extract-i18n-messages.ts",
    "test:watch": "cross-env NODE_ICU_DATA=node_modules/full-icu vitest",
    "e2e": "cypress run --headless",
    "lint": "eslint . --ext ts,vue --fix --cache && tsc --project ./tsconfig.json --noEmit",
    "storybook:build": "storybook build",
    "release:major": "ts-node ./tools/release.ts --major",
    "release:minor": "ts-node ./tools/release.ts --minor",
    "release:patch": "ts-node ./tools/release.ts --patch",
    "build": "nuxi build",
    "build:analyze": "nuxi analyze",
    "build:spa": "nuxi generate",
    "start": "cross-env NODE_ICU_DATA=node_modules/full-icu node .output/server/index.mjs",
    "prepare": "husky install",
    "pre-commit": "pretty-quick --staged && concurrently \"npm run lint\" \"npm test\""
  },
  "dependencies": {
    "@firebase/app-types": "0.9.0",
    "@pinia/nuxt": "0.4.11",
    "@vee-validate/rules": "4.9.6",
    "@vueuse/core": "10.2.0",
    "animejs": "3.2.1",
    "bcrypt": "5.1.0",
    "cross-env": "7.0.3",
    "firebase": "10.1.0",
    "firebase-admin": "11.10.1",
    "firebase-functions": "4.4.1",
    "full-icu": "1.5.0",
    "lodash-es": "4.17.21",
    "marked": "5.1.0",
    "mitt": "3.0.0",
    "next-auth": "4.22.1",
    "pinia": "2.1.4",
    "vee-validate": "4.9.6"
  },
  "devDependencies": {
    "@babel/core": "7.22.5",
    "@commitlint/config-conventional": "17.6.5",
    "@nuxt/devtools": "0.6.1",
    "@nuxt/image": "1.0.0-rc.1",
    "@nuxtjs/color-mode": "3.2.0",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/html-validator": "1.4.0",
    "@nuxtjs/i18n": "8.0.0-beta.10",
    "@nuxtjs/robots": "3.0.0",
    "@sidebase/nuxt-auth": "0.5.0",
    "@storybook/addon-a11y": "7.2.3",
    "@storybook/addon-actions": "7.2.3",
    "@storybook/addon-essentials": "7.2.3",
    "@storybook/addon-interactions": "7.2.3",
    "@storybook/addon-links": "7.2.3",
    "@storybook/addon-mdx-gfm": "7.2.3",
    "@storybook/testing-library": "0.2.0",
    "@storybook/vue3": "7.2.3",
    "@storybook/vue3-vite": "7.2.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/vue": "7.0.0",
    "@types/animejs": "3.1.7",
    "@types/bcrypt": "5.0.0",
    "@types/lodash-es": "4.17.7",
    "@types/marked": "5.0.0",
    "@typescript-eslint/eslint-plugin": "5.59.11",
    "@typescript-eslint/parser": "5.59.11",
    "@vite-pwa/nuxt": "0.1.0",
    "@vitejs/plugin-vue": "4.2.3",
    "@vitest/coverage-v8": "0.32.2",
    "@vuesion/storybook-theme-switcher": "5.1.1-beta.0",
    "babel-loader": "9.1.2",
    "c8": "8.0.0",
    "concurrently": "8.2.0",
    "cypress": "12.14.0",
    "eslint": "8.42.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-storybook": "0.6.13",
    "eslint-plugin-vue": "9.14.1",
    "flush-promises": "1.0.2",
    "generate-changelog": "1.8.0",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "hygen": "6.2.11",
    "jsdom": "22.1.0",
    "nuxt": "3.5.3",
    "nuxt-delay-hydration": "1.2.1",
    "nuxt-security": "0.13.0",
    "openapi-typescript": "5.4.1",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "pretty-quick": "3.1.3",
    "sass": "1.63.4",
    "sass-loader": "13.3.2",
    "storybook": "7.2.3",
    "ts-node": "10.9.1",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-plugin-eslint": "1.8.1",
    "vitest": "0.32.2",
    "vue-loader": "17.2.2"
  },
  "peerDependencies": {
    "consola": "3.2.3",
    "vue": "3.3.4"
  }
}
